<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="gravity"
    android:textDirection="firstStrong"
    android:visibility="visible"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/image"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="37dp"
        tools:ignore="MissingConstraints">
    </ImageView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotationX="4"
        android:text="TextView"
        android:textAlignment="textStart"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#101010"
        android:textDirection="firstStrong"
        android:textStyle="bold"
        android:visibility="visible"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="425dp"
        tools:text="Apple - 13"
        tools:visibility="visible"
        tools:ignore="MissingConstraints">
    </TextView>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Fcfa 1202943,08"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#101010"
        android:textDirection="firstStrong"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:autoSizeMinTextSize="13dp"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="438dp"
        tools:visibility="visible"
        tools:ignore="MissingConstraints">
    </TextView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Les points forts"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#03A9F4"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="507dp"
        tools:text="Les points forts"
        tools:ignore="MissingConstraints">
    </TextView>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Usage:"
        android:textColor="#222121"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="552dp"
        tools:ignore="MissingConstraints">
    </TextView>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:hint="13"
        android:text="Ultraportable &amp; Performant"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="552dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RAM:"
        android:textColor="#171616"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="604dp"
        tools:ignore="MissingConstraints">
    </TextView>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="16 Go (la mémoire fournie est soudée)"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="604dp"
        tools:ignore="MissingConstraints">
    </TextView>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Savoir plus"
        android:textColor="#E91E63"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="641dp"
        tools:ignore="MissingConstraints">
    </TextView>

    <com.google.android.material.chip.Chip
        android:id="@+id/chip4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@android:color/holo_green_light"
        android:fontFamily="sans-serif-medium"
        android:text="Valider"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:visibility="visible"
        app:chipStrokeColor="#8BC34A"
        app:chipStrokeWidth="3dp"
        app:chipSurfaceColor="@android:color/holo_green_light"
        app:rippleColor="@android:color/holo_green_light"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints">
    </com.google.android.material.chip.Chip>

    <com.google.android.material.chip.Chip
        android:id="@+id/chip5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modifier"
        android:textColor="@color/white"
        app:chipSurfaceColor="@android:color/holo_blue_dark"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="679dp"></com.google.android.material.chip.Chip>

    <com.google.android.material.chip.Chip
        android:id="@+id/chip6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Supprimer"
        android:textColor="@color/white"
        app:chipSurfaceColor="@android:color/holo_red_dark"
        tools:layout_editor_absoluteX="306dp"
        tools:layout_editor_absoluteY="679dp"
        tools:ignore="MissingConstraints">
    </com.google.android.material.chip.Chip>

</androidx.constraintlayout.widget.ConstraintLayout>